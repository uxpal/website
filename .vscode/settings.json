{
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": true
    },
    "editor.formatOnSave": false,
    "[json]": {
        "editor.formatOnSave": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "typescriptreact"
    ],
    "eslint.options": {
        "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".md",
            ".mdx"
        ]
    },
    "workbench.colorCustomizations": {
            "sideBarSectionHeader.background": "#298f0d"
    }
}
